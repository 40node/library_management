@startuml database
package database <<Database>> {
  entity users {
    + id
    --
    name
    - auth_hash
    ..
    created_at
    updated_at
  }
  entity libraries {
    + id
    --
    book_title
    author
    publisher
    isbn_10
    isbn_13
    image_url
    ..
    created_at
    updated_at
  }
}
users -ri-|{ libraries : 所持する本 >
@enduml